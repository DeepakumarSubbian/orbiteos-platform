        },
        {
          "datasource": { "type": "postgres", "uid": "TimescaleDB" },
          "format": "time_series",
          "rawSql": "SELECT time_bucket('5 minutes', time) AS time, avg(value) as \"Grid\" FROM device_telemetry WHERE metric_name = 'grid_power' AND time > now() - interval '24 hours' GROUP BY 1 ORDER BY 1",
          "refId": "C"
        },
        {
          "datasource": { "type": "postgres", "uid": "TimescaleDB" },
          "format": "time_series",
